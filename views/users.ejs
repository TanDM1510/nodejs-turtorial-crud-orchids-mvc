<div
  class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
>
  <h1 class="h2">Dashboard</h1>
  <a href="/logout" class="btn btn-sm btn-label-danger">Log out</a>
</div>

<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th scope="col">User name</th>
        <th scope="col">name</th>
        <th scope="col">YOB</th>
        <th scope="col">Is Admin</th>
      </tr>
    </thead>

    <tbody>
      <% orchids.forEach(element => { %>
      <tr class="align-middle">
        <td><%= element.username %></td>

        <td><%= element.name %></td>
        <td><%= element.YOB %></td>

        <td><%= element.isAdmin ? 'Yes' : 'No' %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<% if (orchids.length > 0) { %>
<nav aria-label="Dashboard Pagination " style="margin-top: 100px">
  <ul class="pagination justify-content-center mt-5">
    <% if (current == 1) { %>
    <li class="page-item disabled"><a href="#" class="page-link">First</a></li>
    <% } else { %>
    <li class="page-item">
      <a href="/accounts/?page=1" class="page-link">First</a>
    </li>
    <% } %> <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %> <%
    if(i !== 1) { %>
    <li class="page-item disabled"><a href="#" class="page-link">...</a></li>
    <% } %> <% for(; i <= (Number(current) + 4) && i <= pages; i++ ) { %> <% if
    (i == current) { %>
    <li class="page-item disabled">
      <a href="#" class="page-link"><%= i %></a>
    </li>
    <% } else { %>
    <li class="page-item">
      <a href="/accounts/?page=<%= i %>" class="page-link"><%= i %></a>
    </li>
    <% } %> <% if (i == Number(current) + 4 && i < pages) { %>
    <li class="page-item disabled"><a href="#" class="page-link">...</a></li>
    <% } %> <% } %> <% if (current == pages) { %>
    <li class="page-item disabled"><a href="#" class="page-link">Last</a></li>
    <% } else { %>
    <li class="page-item">
      <a href="/accounts/?page=<%= pages %>" class="page-link">Last</a>
    </li>
    <% } %>
  </ul>
</nav>
<% } %>
